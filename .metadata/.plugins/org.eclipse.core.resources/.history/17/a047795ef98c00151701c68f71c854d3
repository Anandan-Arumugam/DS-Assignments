package com.anandan;

public class BindingAndOverloading {

	  public boolean equalsa( BindingAndOverloading other ) {
	    System.out.println( "Inside of Test.equals" );
	    return false;
	  }

	  public static void main( String [] args ) {
	    Object t1 = new BindingAndOverloading();
	    Object t2 = new BindingAndOverloading();
	    BindingAndOverloading t3 = new BindingAndOverloading();
	    Object o1 = new Object();

	    int count = 0;
	    System.out.println( count++ );// prints 0
	    t1.equalsa( t2 ) ;
	    System.out.println( count++ );// prints 1
	    t1.equalsa( t3 );
	    System.out.println( count++ );// prints 2
	    t3.equalsa( o1 );
	    System.out.println( count++ );// prints 3
	    t3.equalsa(t3);
	    System.out.println( count++ );// prints 4
	    t3.equalsa(t2);
	  }
	}
